net: "models/entropy/drsae_train_test.prototxt"
test_iter: 100
test_interval: 500

base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005

lr_policy: "step"
gamma: 0.1
stepsize: 500000

display: 100

max_iter: 1000000

snapshot: 10000
snapshot_prefix: "snapshot/drsae/drsae_v00" # entropy
#snapshot_prefix: "snapshot/drsae/drsae_v01" # logistic

solver_mode: CPU

#clip_gradients: 10
