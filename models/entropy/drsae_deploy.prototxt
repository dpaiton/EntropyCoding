name: "DrSAE"
input: "data"
input_shape {
    dim: 1
    dim: 1 
    dim: 28
    dim: 28
}

layer {
  name: "ip0"
  type: "InnerProduct"
  bottom: "data"
  top: "ip0"
  param {
    lr_mult: 0
    decay_mult: 0
    name: "w0"
  }
  param {
    lr_mult: 0
    decay_mult: 0
    name: "b0"
  }
  inner_product_param {
    num_output: 400
    weight_filler {
      type: "constant"
      value: 0
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "Ex"
  type: "InnerProduct"
  bottom: "data"
  top: "Ex"
